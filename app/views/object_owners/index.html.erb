<p id="notice"><%= notice %></p>

<h1 align="center">Virtual Machine Owners</h1>

<table class="table">
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @object_owners.each do |object_owner| %>
      <tr>
        <td><%= object_owner.firstname %></td>
        <td><%= object_owner.lastname %></td>
        <td><%= object_owner.email %></td>
        <td><%= link_to "View", object_owner, class: "btn btn-success-outline" %></td>
        <td><%= link_to "Edit", edit_object_owner_path(object_owner), class: "btn btn-success-outline" %></td>
        <td><%= link_to "Delete", object_owner, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-success-outline" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to "New Owner", new_object_owner_path, class: "btn btn-success-outline" %>
